{
  "name": "glitch-hello-eleventy",
  "version": "0.1.0",
  "description": "A simple blog, set up with Eleventy.",
  "scripts": {
    "start": "postcss style.css > build/public/style.css && eleventy --serve",
    "build": "NODE_ENV=production eleventy && NODE_ENV=production postcss style.css > build/public/style.css"
  },
  "dependencies": {
    "eleventy-plugin-seo": "0.5.2",
    "luxon": "^3.4.0",
    "tailwindcss": "^3.4.1",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "autoprefixer": "^10.4.17",
    "cssnano": "^6.0.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0"
  },
  "engines": {
    "node": "16"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/glitch-hello-eleventy"
  },
  "glitch": {
    "projectType": "generated_static"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ]
}
